# ASEAGI Environment Configuration
# Copy to .env and fill in your values

# Supabase
SUPABASE_URL=https://jvjlhxodmbkodzmggwpu.supabase.co
SUPABASE_KEY=your-supabase-anon-key

# Anthropic Claude
ANTHROPIC_API_KEY=your-claude-api-key

# Vast.ai
VASTAI_API_KEY=your-vastai-api-key

# DigitalOcean Spaces (S3-compatible)
S3_ENDPOINT=https://nyc3.digitaloceanspaces.com
S3_ACCESS_KEY=your-spaces-access-key
S3_SECRET_KEY=your-spaces-secret-key
S3_BUCKET=aseagi-documents
S3_REGION=nyc3

# Telegram Bot
TELEGRAM_BOT_TOKEN=your-telegram-bot-token

# Droplet Configuration
DROPLET_IP=your-droplet-ip-address
DOMAIN=aseagi.yourdomain.com  # optional

# Docker Registry (for Vast.ai image)
DOCKER_USERNAME=your-dockerhub-username
DOCKER_IMAGE=aseagi/document-processor
