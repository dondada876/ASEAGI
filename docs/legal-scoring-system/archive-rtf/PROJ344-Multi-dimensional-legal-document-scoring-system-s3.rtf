\uc0\u9553                                                                  \u9553 \
\uc0\u9553   1. EVIDENCE STRENGTH (0-100)                                  \u9553 \
\uc0\u9553      \u9500 \u9472  Direct Evidence: Weight 40%                             \u9553 \
\uc0\u9553      \u9500 \u9472  Circumstantial Evidence: Weight 30%                     \u9553 \
\uc0\u9553      \u9500 \u9472  Witness Credibility: Weight 20%                         \u9553 \
